#include<stdio.h>
int main()
{
	int n,a;
	scanf("%d",&n);
	while(n--)
	{
		scanf("%d",&a);
		int b[a];
		int sum1=0,sum2=0;
		for(int i=1;i<=a;++i)
		{
			scanf("%d",&b[i]);

			if(b[i]%2==0)
			{
				sum1+=b[i];
			}
			else
			{
				sum2+=b[i];
			}
		}
		if(sum1>sum2)
			printf("Yes\n");
		else
			printf("No\n");

	}

	return 0;
}
